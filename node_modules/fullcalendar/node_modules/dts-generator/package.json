    "glob": "7.0.0",
    "mkdirp": "0.5.1"
  },
  "devDependencies": {
    "dojo-loader": ">=2.0.0-beta.5",
    "intern": "~3.0.0",
    "tsd": "0.6.5",
    "tslint": "3.11.0",
